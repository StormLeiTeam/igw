<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="StatusBarView" >

        <attr name="statusBarVisible" format="boolean" />
        <attr name="appActionBarVisible" format="boolean" />
        <attr name="backVisible" format="boolean"/>
        <attr name="menuVisible" format="boolean"/>
        <attr name="doVisible" format="boolean" />
        <attr name="backImageVisible" format="boolean"/>
        <attr name="backTitleVisible" format="boolean"/>
        <attr name="menuImageVisible" format="boolean"/>
        <attr name="menuTitleVisible" format="boolean"/>
        <attr name="isShowMenu" format="boolean" />
        <attr name="showTitle" format="boolean" />

        <attr name="shareVisible" format="boolean" />
        <attr name="confirmVisible" format="boolean" />



        <attr name="statusBarColor" format="color"/>
        <attr name="appActionBarColor" format="color"/>

        <attr name="backTextColor" format="color" />
        <attr name="menuTextColor" format="color" />
        <attr name="subTitleColor" format="color" />
