        <attr name="subTitleRes" format="reference" />
        <attr name="doTitleRes" format="reference" />
        <attr name="shareRes" format="reference" />

        <attr name="backText" format="string" />
        <attr name="menuText" format="string"/>
        <attr name="statusTitle" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="confirmText" format="string" />




